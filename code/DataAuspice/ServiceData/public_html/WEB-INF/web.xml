    <servlet-class>mx.net.cablevision.service.DasNOCGetStatusEquipment</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>DasNOCGetStatusEquipmentPort</servlet-name>
    <url-pattern>/DasNOCGetStatusEquipmentPort</url-pattern>
  </servlet-mapping>
</web-app>
